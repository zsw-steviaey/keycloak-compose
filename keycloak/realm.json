{
  "id": "${KC_REALM_NAME}",
  "realm": "${KC_REALM_NAME}",
  "sslRequired": "none",
  "enabled": true,
  "eventsEnabled": true,
  "eventsExpiration": 900,
  "adminEventsEnabled": true,
  "adminEventsDetailsEnabled": true,
  "attributes": {
    "adminEventsExpiration": "900"
  },
  "clients": [
    {
      "id": "grafana",
      "clientId": "grafana",
      "name": "grafana",
      "enabled": true,
      "publicClient": true,
      "rootUrl": "${GF_URL}",
      "adminUrl": "${GF_URL}",
      "baseUrl": "${GF_URL}",
      "redirectUris": [
        "${GF_URL}/*"
      ],
      "webOrigins": [
        "${GF_URL}"
      ]
    }
  ],
  "users": [
    {
      "id": "admin",
      "email": "${GF_ADMIN_USERNAME}@admin",
      "username": "${GF_ADMIN_USERNAME}",
      "firstName": "DevOps",
      "lastName": "SRE",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "temporary": false,
          "type": "password",
          "value": "${GF_ADMIN_PASSWORD}"
        }
      ]
    }
  ]
}
